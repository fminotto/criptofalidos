<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nao_cancelar_ordem_executada</fullName>
    <active>false</active>
    <errorConditionFormula>ISPICKVAL(Status__c, &apos;Cancelada&apos;) &amp;&amp;  NOT(ISNULL( Data_Execucao__c ))</errorConditionFormula>
    <errorMessage>Não é possível cancelar uma ordem já executada</errorMessage>
</ValidationRule>
